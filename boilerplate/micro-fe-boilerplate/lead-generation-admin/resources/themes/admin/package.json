    "start:staging": "NODE_ENV=staging webpack serve --mode=staging",
    "start:prod": "NODE_ENV=production webpack serve --mode=production",
    "lint": "vue-cli-service lint",
    "popup": "cp public/popup.dev.v2.1.js public/popup.js ",
    "js": "uglifyjs ./public/popup.dev.js -o ./public/popup.js --mangle-props keep_quoted -c -m",
